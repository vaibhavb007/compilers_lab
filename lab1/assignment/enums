        enum Tokens{INT=0x100,
        			FLOAT,
                                VOID,
                                IDENTIFIER,
        			INT_CONSTANT,
        			FLOAT_CONSTANT,
        			RETURN,
        			OR_OP,
        			AND_OP,
        			EQ_OP,
        			NE_OP,
        			LE_OP,
        			GE_OP,
        			INC_OP,
        			IF,
        			ELSE,
        			WHILE,
        			FOR,
        			OTHERS,
                                STRING_LITERAL};
